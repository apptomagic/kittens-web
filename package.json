{
  "name": "kittens-web-root",
  "private": true,
  "scripts": {
    "start": "cd packages/kittens-web-server && npm start",
    "debug": "cd packages/kittens-web-server && npm run debug",
    "prettier-check": "prettier --check **/*.js",
    "prettier-fix": "prettier --write **/*.js"
  },
  "devDependencies": {
    "husky": "^2.4.1",
    "lerna": "^3.15.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx -p prettier -p pretty-quick pretty-quick --staged"
    }
  }
}
